void setup() {

#ifdef ROTATE_PANEL
  hw.config.RotatePanel = false;
#endif
#ifdef REVERSE_ENCODER
  hw.config.ReverseEncoder = true;
#endif

  hw.Init();
  hw.display.clearDisplay();

    // Display text on the bottom row (y coordinate might need adjustment based on your screen)
  hw.display.setTextColor(WHITE);
  hw.display.setCursor(18, 20);  // Adjust 54 if necessary
  hw.display.print("Cat");
  hw.display.setCursor(16, 35);  // Adjust 54 if necessary
  hw.display.setTextSize(1);    // Set text size to 1 (default size)
  hw.display.print("Muybridge (1887)");
  hw.display.display();

  delay(2000);
}

void loop() {
  hw.ProcessInputs();

  bool rst = hw.rst.State() == DigitalInput::STATE_RISING;
  bool clk = hw.clk.State() == DigitalInput::STATE_RISING;

  if (rst) resetSeq();

  if (clk) {
    lastClockTime = millis();
    useInternalClock = false;
    advanceFrame();
  }

  if (millis() - lastClockTime > 3000) {
    useInternalClock = true;
  }

  if (useInternalClock) {
    static unsigned long lastInternalTime = 0;
    if (millis() - lastInternalTime > 20) {
      lastInternalTime = millis();
      advanceFrame();
    }
  }
}

void advanceFrame() {
  framecount = (framecount + 1) % 24;

  hw.display.clearDisplay();
  hw.display.fillRect(16, 8, 96, 48, BLACK);
  hw.display.drawBitmap(16, 8, cat2_frame_s[framecount], 96, 48, WHITE);

  // Adjusted triggering based on paw placement
  switch (framecount) {
    case 0: case 6: case 12: case 18:
      triggerOutputs(1, 0, 0, 1); break;
    case 3: case 9: case 15: case 21:
      triggerOutputs(0, 1, 1, 0); break;
    case 5: case 11: case 17: case 23:
      triggerOutputs(1, 1, 0, 0); 
      hw.outputs[5].High();
      hw.outputs[2].Low();
      break;
    case 7: case 13: case 19:
      triggerOutputs(0, 1, 0, 1); break;
    case 10: case 16: case 22:
      triggerOutputs(0, 0, 1, 0); break;
  }

  hw.display.display();
}

void resetSeq() {
  framecount = 0;
}

void triggerOutputs(byte hoof1, byte hoof2, byte hoof3, byte hoof4) {
  if (hoof1 == 1) hw.outputs[0].High();
  else hw.outputs[0].Low();

  if (hoof2 == 1) hw.outputs[1].High();
  else hw.outputs[1].Low();

  if (hoof3 == 1) hw.outputs[3].High();
  else hw.outputs[3].Low();

  if (hoof4 == 1) hw.outputs[4].High();
  else hw.outputs[4].Low();
}
