void setup() {
#ifdef ROTATE_PANEL
  hw.config.RotatePanel = false;
#endif
#ifdef REVERSE_ENCODER
  hw.config.ReverseEncoder = true;
#endif

  hw.Init();

  drawAnimation();
  delay(1200);

  hw.display.clearDisplay();
  hw.display.setTextColor(WHITE);
  hw.display.setCursor(18, 20);
  hw.display.print("Kissing Couple");
  hw.display.setCursor(16, 35);
  hw.display.setTextSize(1);
  hw.display.print("Love Animation");
  hw.display.display();

  delay(2000);
}

void loop() {
  hw.ProcessInputs();

  bool rst = hw.rst.State() == DigitalInput::STATE_RISING;
  bool clk = hw.clk.State() == DigitalInput::STATE_RISING;

  // Reset sequence on reset input
  if (rst) {
    resetSeq();
  }

  // Check for clock pulse and update the lastClockTime
  if (clk) {
    lastClockTime = millis();
    useInternalClock = false;  // Use external clock
    advanceFrame();
  }

  // Check if 3 seconds have passed since the last clock pulse
  if (millis() - lastClockTime > 3000) {
    useInternalClock = true;  // Switch to internal clocking
  }

  // If using internal clock, advance the frame every 66ms for ~15fps
  if (useInternalClock) {
    static unsigned long lastInternalTime = 0;
    if (millis() - lastInternalTime > 66) {
      lastInternalTime = millis();
      advanceFrame();
    }
  }
}

void advanceFrame() {
  framecount = (framecount + 1) % kisss_LEN;

  hw.display.clearDisplay();
  hw.display.fillRect(0, 0, 128, 64, BLACK);
  hw.display.drawBitmap(40, 0, kisss[framecount], 48, 60, WHITE);

  // Handle kissing state (frames 4-9)
  if (framecount >= 4 && framecount <= 9) {
    if (kissingState) {
      hw.outputs[0].High();  // Turn on output 1
      hw.outputs[3].Low();   // Turn off output 4
    } else {
      hw.outputs[0].Low();   // Turn off output 1
      hw.outputs[3].High();  // Turn on output 4
    }
    kissingState = !kissingState;  // Toggle the state for next frame
  } else {
    hw.outputs[0].Low();  // Turn off output 1
    hw.outputs[3].Low();  // Turn off output 4
  }

  // Trigger outputs 2 and 5 on frames 0 and 1
  if (framecount == 0 || framecount == 1) {
    hw.outputs[1].High();  // Turn on output 2
    hw.outputs[4].High();  // Turn on output 5
  } else {
    hw.outputs[1].Low();  // Turn off output 2
    hw.outputs[4].Low();  // Turn off output 5
  }

  // Toggle outputs 3 and 6 each "kissing cycle"
  if (framecount == 9) {  // End of a kissing cycle
    kissCycle++;
    if (kissCycle % 2 == 0) {
      hw.outputs[2].High();  // Turn on output 3
      hw.outputs[5].Low();   // Turn off output 6
    } else {
      hw.outputs[2].Low();   // Turn off output 3
      hw.outputs[5].High();  // Turn on output 6
    }
  }

  hw.display.display();
}

void drawAnimation() {
  hw.display.fillRect(0, 0, 128, 64, BLACK);
  hw.display.drawBitmap(50, 16, Modulove_Logo, 28, 32, WHITE);
  hw.display.display();
}

void resetSeq() {
  framecount = 0;
  kissCycle = 0;
  kissingState = false;
  hw.outputs[0].Low();
  hw.outputs[1].Low();
  hw.outputs[2].Low();
  hw.outputs[3].Low();
  hw.outputs[4].Low();
  hw.outputs[5].Low();
}
