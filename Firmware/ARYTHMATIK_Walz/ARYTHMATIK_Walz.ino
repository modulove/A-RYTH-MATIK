void loop() {
    hw.ProcessInputs();

    bool rst = hw.rst.State() == DigitalInput::STATE_RISING;
    bool clk = hw.clk.State() == DigitalInput::STATE_RISING;

    // Reset sequence on reset input
    if (rst) {
        resetSeq();
    }

    // Check for clock pulse and update the lastClockTime
    if (clk) {
        lastClockTime = millis();
        useInternalClock = false;  // Use external clock
        advanceFrame();
    }

    // Check if 3 seconds have passed since the last clock pulse
    if (millis() - lastClockTime > 3000) {
        useInternalClock = true;  // Switch to internal clocking
    }

    // If using internal clock, advance the frame every 20ms
    if (useInternalClock) {
        static unsigned long lastInternalTime = 0;
        if (millis() - lastInternalTime > 200) {
            lastInternalTime = millis();
            advanceFrame();
        }
    }
}

void advanceFrame() {
    framecount = (framecount + 1) % walzer_LEN;

    hw.display.clearDisplay();
    hw.display.fillRect(0, 0, 128, 64, BLACK);
    hw.display.drawBitmap(36, 2, walzer[framecount], 56, 60, WHITE);

    // Trigger outputs based on a 3/4 waltz pattern
    switch (framecount % 3) {
        case 0:
            triggerOutputs(1, 0, 0, 0);  // Strong beat
            break;
        case 1:
            triggerOutputs(0, 1, 0, 0);  // Light beat
            break;
        case 2:
            triggerOutputs(0, 0, 1, 0);  // Light beat
            break;
    }

    // Toggle outputs 5 and 6 on specific frames
    if (framecount % 3 == 0) {
        hw.outputs[5].High();
        hw.outputs[4].Low();
    } else {
        hw.outputs[5].Low();
        hw.outputs[4].High();
    }

    hw.display.display();
}

void resetSeq() {
    framecount = 0;
}

void triggerOutputs(byte out1, byte out2, byte out3, byte out4) {
    if (out1 == 1)
        hw.outputs[0].High();
    else
        hw.outputs[0].Low();

    if (out2 == 1)
        hw.outputs[1].High();
    else
        hw.outputs[1].Low();

    if (out3 == 1)
        hw.outputs[2].High();
    else
        hw.outputs[2].Low();

    if (out4 == 1)
        hw.outputs[3].High();
    else
        hw.outputs[3].Low();
}
